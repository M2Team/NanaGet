                </VisualStateGroup.Transitions>
                <VisualState x:Name="CompactClosed" />
                <VisualState x:Name="CompactOpenUp" />
                <VisualState x:Name="CompactOpenDown" />
              </VisualStateGroup>
              <VisualStateGroup x:Name="ExpansionStates">
