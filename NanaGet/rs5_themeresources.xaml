                HorizontalAlignment="Center"
                VerticalAlignment="Center">
                <ContentPresenter
                  x:Name="Icon"
                  Content="{TemplateBinding Icon}"
                  Foreground="{ThemeResource DefaultTextForegroundThemeBrush}" />
              </Viewbox>
              <ContentPresenter
                x:Name="ContentPresenter"
                Grid.Column="1"
                Margin="8,0,16,0"
