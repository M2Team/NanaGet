              <VisualStateGroup x:Name="SelectionStates">
                <VisualStateGroup.Transitions>
                  <VisualTransition
                    GeneratedDuration="0:0:0.33"
                    From="Unselected"
                    To="UnselectedLocked" />
                  <VisualTransition
                    GeneratedDuration="0:0:0.33"
                    From="UnselectedLocked"
                    To="Unselected" />
                </VisualStateGroup.Transitions>
                <VisualState x:Name="Disabled">
                  <VisualState.Setters>
