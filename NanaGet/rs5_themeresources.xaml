                <Rectangle
                  x:Name="SwitchKnobOff"
                  Width="12"
                  Height="12"
                  Margin="-1,0,0,0"
                  HorizontalAlignment="Center"
                  Fill="{ThemeResource ToggleSwitchKnobFillOff}"
                  RadiusX="7"
                  RadiusY="7"
                  RenderTransformOrigin="0.5, 0.5">
                  <Rectangle.RenderTransform>
                    <CompositeTransform />
